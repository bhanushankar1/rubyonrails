<p id="notice"><%= notice %></p>

<h1>Mrks</h1>

<table>
  <thead>
    <tr>
      <th>M1</th>
      <th>M2</th>
      <th>M3</th>
      <th>M4</th>
      <th>M5</th>
      <th>Total</th>
      <th>Percent</th>
      <th>Rank</th>
      <th>Schol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mrks.each do |mrk| %>
      <tr>
        <td><%= mrk.m1 %></td>
        <td><%= mrk.m2 %></td>
        <td><%= mrk.m3 %></td>
        <td><%= mrk.m4 %></td>
        <td><%= mrk.m5 %></td>
        <td><%= mrk.total %></td>
        <td><%= mrk.percent %></td>
        <td><%= mrk.rank %></td>
        <td><%= mrk.schol_id %></td>
        <td><%= link_to 'Show', mrk %></td>
        <td><%= link_to 'Edit', edit_mrk_path(mrk) %></td>
        <td><%= link_to 'Destroy', mrk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mrk', new_mrk_path %>
